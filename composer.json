{
	"name": "kaneki/laravel-addons",
	"type": "framework-extension",
	"description": "Addons Extension Pack for Laravel",
	"license": "MIT",
	"keywords": ["laravel", "laravel addons", "laravel extension"],
	"homepage": "https://github.com/kaneki/laravel-addons",
	"authors": [
		{
			"name": "JYmusic",
			"email": "zhangcb1984@163.com"
		},
		{
			"name": "Kaneki",
			"email": "zhangkaneki@gmail.com"
		}
	],
	"require": {
		"php": "^7.2.5",
        	"laravel/framework": "~5.5|^6.0|^7.0|^8.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1"
    },
	"autoload": {
		"psr-4": {
			"Kaneki\\LaravelAddons\\": "src/"
		},
		"files": [
			"src/Helpers.php"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"Kaneki\\LaravelAddons\\ServiceProvider"
			],
			"aliases": {
			}
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
